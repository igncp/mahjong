{
  "name": "mahjong-web-client",
  "private": true,
  "scripts": {
    "build": "rm -rf out .next && next build && next export",
    "dev": "next dev -p 9000",
    "i18next": "i18next",
    "lint": "prettier --check 'src/**' 'pages/**' && eslint . && npm run lint:styles",
    "lint:styles": "stylelint 'src/**/*.css'",
    "lint:fix": "prettier --write 'src/**' 'pages/**' && eslint --fix .",
    "sync_sdk": "cd ../ts_sdk && npm run pack-full && cd ../web_client && npm install ../ts_sdk/*.tgz",
    "install:initial": "npm uninstall mahjong_sdk && npm i && npm run sync_sdk",
    "test": "i18next && grep '\"\"' -r public/locales | wc -l | grep -q 0",
    "e2e": "playwright test",
    "e2e:prod": "E2E_BASE_URL=https://mahjong-rust.com playwright test",
    "typecheck": "tsc --noEmit --project ."
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^13.2.4",
    "@playwright/test": "^1.35.1",
    "@types/qs": "^6.9.7",
    "antd": "^5.4.2",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "formik": "^2.4.2",
    "graphql": "^16.7.1",
    "graphql-request": "^6.1.0",
    "i18next": "^22.4.15",
    "i18next-http-backend": "^2.2.0",
    "i18next-parser": "^8.0.0",
    "jsonwebtoken": "^9.0.0",
    "mahjong_sdk": "file:../ts_sdk/mahjong_sdk-0.1.0.tgz",
    "next": "^13.2.4",
    "qs": "^6.11.1",
    "rdndmb-html5-to-touch": "^8.0.0",
    "react": "18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-multi-backend": "^8.0.0",
    "react-dnd-preview": "^8.0.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.2",
    "rxjs": "^7.8.0",
    "stylelint": "^15.6.2",
    "stylelint-scss": "^5.0.0",
    "web_lib": "file:pkg",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "18.15.9",
    "@types/react": "18.0.29",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
    "sass": "^1.62.1",
    "typescript": "5.0.2"
  }
}
